      "Nip\\View\\Tests\\Fixtures\\App\\": "tests/fixtures/app"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/container": "^1.0",
    "bytic/collections": "^1.0",
    "bytic/helpers": "^1.0",
    "bytic/pipeline": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  }
}
